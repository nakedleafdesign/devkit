<%
if (typeof page === 'undefined') {
  var page = {
    title:'default',
    url:'default',
    date: 'default'
  };
}
%>
<section class="key-visual">
  <div class="key-visual__inner">
    <div class="key-visual__unit">
      <h1 class="key-visual__title"><%= page.title %></h1>
      <span class="key-visual__sub"><%= page.date %></span>
    </div>
  </div>
</section>